{
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "command": "npm run build",
    "output": "dist"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "error": {
    "status": 404,
    "message": "Not found"
  }
}
